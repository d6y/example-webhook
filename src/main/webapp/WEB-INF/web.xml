<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
	<servlet>
        <servlet-name>echo</servlet-name>
        <servlet-class>com.taykt.api.examples.webhook.EchoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>date</servlet-name>
        <servlet-class>com.taykt.api.examples.webhook.DateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>poll</servlet-name>
        <servlet-class>com.taykt.api.examples.webhook.DefinedOptionsPollServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>open</servlet-name>
        <servlet-class>com.taykt.api.examples.webhook.OpenPollServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>unique</servlet-name>
        <servlet-class>com.taykt.api.examples.webhook.SingleVotePerMobilePollServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>date</servlet-name>
        <url-pattern>/date</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>echo</servlet-name>
        <url-pattern>/echo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>poll</servlet-name>
        <url-pattern>/poll</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>open</servlet-name>
        <url-pattern>/open</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>unique</servlet-name>
        <url-pattern>/unique</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>/index.html</welcome-file>
    </welcome-file-list>
</web-app>